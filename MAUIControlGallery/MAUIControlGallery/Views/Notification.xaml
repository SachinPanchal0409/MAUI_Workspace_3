<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MAUIControlGallery.Views.Notification"
             Title="Notification">
    <VerticalStackLayout>
        <Button
            Text="Notification"
            HorizontalOptions="StartAndExpand"
            Margin="5,0"
            HeightRequest="50"
            WidthRequest="150"
            Command="{Binding GetNotificationCommand}"/>
    </VerticalStackLayout>
</ContentPage>